{
  "name": "beercan",
  "version": "1.0.0",
  "description": "WordPress Theme",
  "main": "./src/index.js",
  "scripts": {
    "dev": "npm run development",
    "development":
      "cross-env NODE_ENV=development webpack --progress --hide-modules --config config/webpack.config.js",
    "prod": "npm run production",
    "production":
      "cross-env NODE_ENV=production webpack --progress --hide-modules --config config/webpack.config.js",
    "start": "npm run development -- --watch --progress --colors"
  },
